	<script language="javascript">
	function UpdateAppInfo()
	{
		//var packansid = "2077364";
		var Mode = "";
		var cbCount = 2;
		
		with (document.formMain)
		{
			if (document.formMain.CompPubDate)
			{
				if (CompPubDate.value != "")
				{
					validateDates(CompPubDate.value, "Complete Status Publish Date", "","MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}
			
			if (document.formMain.DecPubDate)
			{
				if (DecPubDate.value != "")
				{
					if (validateDates(DecPubDate.value, "Decision Publish Date", "", "MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true))
					{
						var dt = new Date();
						if (Date.parse(DecPubDate.value) < Date.parse(dt.toDateString()))
						{
							validateRequired(Decision.value, "Decision");
						}
					}
				}
			}
			
			if (document.formMain.InProcPubDate)
			{
				if (InProcPubDate.value != "")
				{
					validateDates(InProcPubDate.value, "In Process Publish Date", "", "MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}
			
			if (document.formMain.DecDate)
			{
				if (DecDate.value != "")
				{
					validateDates(DecDate.value, "Decision Date", "", "MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}
			
			if (document.formMain.EnrollPubDate)
			{
				if (EnrollPubDate.value != "")
				{
					validateDates(EnrollPubDate.value, "Enrollment Publish Date", "", "MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}
			
			if (document.formMain.EnrollDeadlineDate)
			{
				if (EnrollDeadlineDate.value != "")
				{
					validateDates(EnrollDeadlineDate.value, "Enrollment Deadline Date", "","MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}
			/*
			if (document.formMain.DecisionType2)
			{
				if (DecisionType2.value != "")
				{
					if (DecisionType2.value == DecisionType1.value)
					{
						addError("Both selected DecisionType cannot have same value.","2nd Decision Type");
					}
				}
			}
			if (document.formMain.DecisionType2)
			{
				if (DecisionType2.value != "")
				{
					if (Decision2.value == "")
					{
						addError("Please select 2nd Decision for 2nd DecisionType.","2nd Decision");
					}
				}
			}		
			if (document.formMain.DecisionType1)
			{
				if (DecisionType1.value != "")
				{
					if (Decision.value == "")
					{
						addError("Please select Decision for DecisionType.","Decision");
					}
				}
			}	
			if (document.formMain.Decision && document.formMain.Decision2 )
			{
				if (Decision.value != "" && Decision2.value != "")
				{
					if (DecisionType1.value == "" && DecisionType2.value == "")
					{
						addError("Please select atleast one DecisionType.","Decision Type");
					}
				}
			}		
			if (document.formMain.DecPubDate2)
			{
				if (DecPubDate2.value != "")
				{
					if (validateDates(DecPubDate2.value, "2nd Decision Publish Date", "", "MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true))
					{
						var dt = new Date();
						if (Date.parse(DecPubDate2.value) < Date.parse(dt.toDateString()))
						{
							validateRequired(Decision2.value, "2nd Decision");
						}
					}
				}
			}			
			
			
			if (document.formMain.DecDate2)
			{
				if (DecDate2.value != "")
				{
					validateDates(DecDate2.value, "2nd Decision Date", "", "MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}
			
			if (document.formMain.EnrollPubDate2)
			{
				if (EnrollPubDate2.value != "")
				{
					validateDates(EnrollPubDate2.value, "2nd Enrollment Publish Date", "", "MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}
			
			if (document.formMain.EnrollDeadlineDate2)
			{
				if (EnrollDeadlineDate2.value != "")
				{
					validateDates(EnrollDeadlineDate2.value, "2nd Enrollment Deadline Date", "","MM/DD/YYYY Or MM/DD/YYYY HH:MM:SS AM or PM", true);
				}
			}*/		
			if (gErrString == "")
			{
				action = "ApplicantSummary_Action.asp?Mode=" + Mode + "&CBCount=" + cbCount;
				submit();
			}
			else
			{
				displayErrors();
				gErrString = "";
			}
		}
	}
	
	function viewEventHistory()
	{
	var lngPackAnsID;
	lngPackAnsID = document.formMain.SelectedIDs.value;
	
		//openWindow ("viewEventProfile.asp?InqPackAnsID=" + -2147483640 + "&InqPackageID=" + -2147483640 + "&ApplicantClientID=1947431");
		openWindow ("viewEventProfile.asp?");
	
	}
	function viewDecisionHistory()
	{
	var lngPackAnsID;
	lngPackAnsID = document.formMain.SelectedIDs.value;
	window.open("viewDecisionTrail.asp?PackAnsID=" + lngPackAnsID, "", "scrollbars=yes, resizable=no, status=no, menubar=no, toolbar=no, width=950, height=500,left=200");
	}
	
