<SCRIPT LANGUAGE="JavaScript">
	var queryDesc = new Object(), groupDesc = new Object(), actionDesc = new Object();
	function submitForm()
	{
		if (validate() == false || gErrString !="") {
			displayErrors();					// else, we didn't pass all validaiton, display the errors.
			return false;
		}
		return true;
	}
	function validate()
	{
		var strValue, blnReturn = true, objSearchTypes = document.frmMain.SearchTypes, strSearchType
		gErrString = "";
		
		for (var i = 0; i < objSearchTypes.length; ++i)	{
			if (objSearchTypes[i].checked)	{
				strSearchType = objSearchTypes[i].value;
				break;
			}
		}
		
		if (strSearchType == "query")
		{	
		
			if (document.frmMain.QueryID.selectedIndex == 0)
			{
				document.frmMain.QueryName.value = document.frmMain.SysQuery.options[document.frmMain.SysQuery.selectedIndex].text.replace(/\xA0/g,'').replace(/^\s+|\s+$/g,'');
			}
			else
			{
				document.frmMain.QueryName.value = document.frmMain.QueryID.options[document.frmMain.QueryID.selectedIndex].text.replace(/\xA0/g,'').replace(/^\s+|\s+$/g,'');
			}
			if ((document.frmMain.SysQuery.selectedIndex == 0 ) && (document.frmMain.QueryID.selectedIndex == 0))
			{
				addError("Select a system query or user-defined query from the drop-down list." , "System Query Or User Query", "System Query Or User Query");
				blnReturn = false;
			}
			if (document.frmMain.SysQuery.selectedIndex > 1) 
			{
				if (trim(document.frmMain.SysQuery.value) == "2" || document.frmMain.SysQuery.value =="3" || document.frmMain.SysQuery.value=="4" || document.frmMain.SysQuery.value=="8")
					document.frmMain.action="search_for_student_systemQuery.asp"
				
			}
		
			//Added by Lalitha on 11/1/01 
			//To assign the value of IsQuery to Query when the user selects the query to run the search
			if (blnReturn) 
			{
				document.frmMain.IsQuery.value = "Query";
			}
		} else if (strSearchType == "individual")
			{
			// Trim the inputs			
			if ("Admissions" == "Inquiry")
			{
				var strDelim = '', strApplyYourselfID = document.frmMain.ApplyyourSelfID.value;					
				if (strApplyYourselfID.search(",") > -1) {
					strDelim = ',';
				} else if (strApplyYourselfID.search(";") > -1){
					strDelim = ';';
				}
				if (strDelim != "") {						
					var arrApplyYourselfIDs = strApplyYourselfID.split(strDelim);						
					for (var j=0; j<arrApplyYourselfIDs.length; ++j){								
						validateNumberBounds(arrApplyYourselfIDs[j],"","","Prospect ID","",false);
					}
				} else {
					validateNumberBounds(strApplyYourselfID,"","","Prospect ID","",false);
				}
				document.frmMain.FirstName.value = trimString(document.frmMain.FirstName.value)
				document.frmMain.LastName.value = trimString(document.frmMain.LastName.value)
				document.frmMain.BirthDate.value = trimString(document.frmMain.BirthDate.value)
				document.frmMain.Email.value = trimString(document.frmMain.Email.value)
				document.frmMain.CreatedFrom.value = trimString(document.frmMain.CreatedFrom.value)
				document.frmMain.CreatedTo.value = trimString(document.frmMain.CreatedTo.value)
				if (document.frmMain.FirstName.value != "")				
					if (document.frmMain.FirstName.value == "%")
						addError("Please enter the valid wildcard search. Single wildcard character is not allowed","FirstName","")
				if (document.frmMain.LastName.value != "")				
					if (document.frmMain.LastName.value == "%")
						addError("Please enter the valid wildcard search. Single wildcard character is not allowed","LastName","")	
				if (document.frmMain.BirthDate.value != "")	{
					blnReturn = validateDate("Birth Date", document.frmMain.BirthDate.value);
				}
				if (document.frmMain.CreatedFrom.value != "") {
					blnReturn = validateDate("Created Date From", document.frmMain.CreatedFrom.value);
				}
				if (document.frmMain.CreatedTo.value != "") {
					blnReturn = validateDate("Created Date To", document.frmMain.CreatedTo.value);
				}
			
			}
			else
			{
				if ("1" == "1") 
				{
					var strDelim = '', strApplyYourselfID = document.frmMain.ApplyyourSelfID.value;					
					if (strApplyYourselfID.search(",") > -1) {
						strDelim = ',';
					} else if (strApplyYourselfID.search(";") > -1){
						strDelim = ';';
					}
					if (strDelim != "") {						
						var arrApplyYourselfIDs = strApplyYourselfID.split(strDelim);						
						for (var j=0; j<arrApplyYourselfIDs.length; ++j){								
							validateNumberBounds(arrApplyYourselfIDs[j],"","","Applicant ID","",false);
						}
					} else {
						validateNumberBounds(strApplyYourselfID,"","","Applicant ID","",false);
					}
					document.frmMain.FirstName.value = trimString(document.frmMain.FirstName.value)
					document.frmMain.LastName.value = trimString(document.frmMain.LastName.value)
					document.frmMain.Pin.value = trimString(document.frmMain.Pin.value)
					document.frmMain.BirthDate.value = trimString(document.frmMain.BirthDate.value)
					document.frmMain.Email.value = trimString(document.frmMain.Email.value)				
					if (document.frmMain.FirstName.value != "")				
						if (document.frmMain.FirstName.value == "%")
							addError("Please enter the valid wildcard search. Single wildcard character is not allowed","FirstName","")
					if (document.frmMain.LastName.value != "")				
						if (document.frmMain.LastName.value == "%")
							addError("Please enter the valid wildcard search. Single wildcard character is not allowed","LastName","")	
					if (document.frmMain.BirthDate.value != "")	{
						blnReturn = validateDate("Birth Date", document.frmMain.BirthDate.value);
					}
				}
				else
				{
					if ("1" == "5")
					{
						document.frmMain.FirstName.value = trimString(document.frmMain.FirstName.value)
						document.frmMain.LastName.value = trimString(document.frmMain.LastName.value)
						document.frmMain.BirthDate.value = trimString(document.frmMain.BirthDate.value)
						document.frmMain.Email.value = trimString(document.frmMain.Email.value)
						document.frmMain.CreatedFrom.value = trimString(document.frmMain.CreatedFrom.value)
						document.frmMain.CreatedTo.value = trimString(document.frmMain.CreatedTo.value)
						if (document.frmMain.FirstName.value != "")				
							if (document.frmMain.FirstName.value == "%")
								addError("Please enter the valid wildcard search. Single wildcard character is not allowed","FirstName","")
						if (document.frmMain.LastName.value != "")				
							if (document.frmMain.LastName.value == "%")
								addError("Please enter the valid wildcard search. Single wildcard character is not allowed","LastName","")	

						if (document.frmMain.BirthDate.value != "")	{
						blnReturn = validateDate("Birth Date", document.frmMain.BirthDate.value);
						}
						if (document.frmMain.CreatedFrom.value != "") {
							blnReturn = validateDate("Created Date From", document.frmMain.CreatedFrom.value);
						}
						if (document.frmMain.CreatedTo.value != "") {
							blnReturn = validateDate("Created Date To", document.frmMain.CreatedTo.value);
						}					
					
					}
				}
		    }
		  
		} else if (strSearchType == "group")	{
			blnReturn = validateSelectionRequired(document.frmMain.GroupID, "Select a Group")
		} else if (strSearchType == "action")	{
			blnReturn = validateSelectionRequired(document.frmMain.ActionID, "Select a Action")
		} else if (strSearchType == "creationDate")	{
			blnReturn = validateSelectionRequired(document.frmMain.Days, "Select a Creation Date")
		} else if (strSearchType == "submissionDate"){
			blnReturn = validateSelectionRequired(document.frmMain.SubmitDays, "Submission Date")
		}
		
		// Check record type Radio buttons
		if ("Admissions" != "Inquiry")
		{
		    
			if ((!validateRadioButtonRequired(document.frmMain.RecordType, "Record Options")) && blnReturn)	{
				blnReturn = false;
			}
			
		}
		return blnReturn;		// return results
	}

function addProperty(objName, propData)	{
	for (var i=0;i<propData.length;++i) {
		objName[propData[i][0]] = propData[i][1]
	}
} 

function setQueryDesc()	{
	var selValue = document.frmMain.QueryID.options[document.frmMain.QueryID.selectedIndex].value;	
	
	if (selValue == "") {
		document.frmMain.QueryDesc.value = "";
	} else {
		document.frmMain.QueryDesc.value = (queryDesc[selValue]==undefined ? "" : queryDesc[selValue]);
	}	
	
	selectSearchType('query');

	document.frmMain.SysQuery.selectedIndex = 0
	
}

function setGroupDesc()	{
	var selValue = document.frmMain.GroupID.options[document.frmMain.GroupID.selectedIndex].value;	
	
	if (selValue == "")	{
		document.frmMain.GroupDesc.value = "";
	} else {
		document.frmMain.GroupDesc.value = (groupDesc[selValue]==undefined ? "" : groupDesc[selValue]);
	}
	
	selectSearchType('group');
}

function setActionDesc()	{
	var selValue = document.frmMain.ActionID.options[document.frmMain.ActionID.selectedIndex].value;	

	if (selValue == "")	{
		document.frmMain.ActionDesc.value = "";
	} else {
		document.frmMain.ActionDesc.value = actionDesc[selValue];
	}

	selectSearchType('action');	
}
function selectSearchType(name)	{
	var types = document.frmMain.SearchTypes

	for (var i = 0; i < types.length; ++i)	{		
		if (types[i].value == name)	{
			types[i].checked = true;
			break;
		}
	}
}


